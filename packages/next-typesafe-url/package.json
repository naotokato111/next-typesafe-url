{
  "name": "next-typesafe-url",
  "description": "Fully typesafe, JSON serializable, and zod validated URL search params, dynamic route params, and routing for NextJS pages directory",
  "license": "MIT",
  "version": "2.1.3",
  "author": "Ethan Niser",
  "repository": {
    "type": "git",
    "url": "https://github.com/ethanniser/next-typesafe-url"
  },
  "homepage": "https://next-typesafe-url.dev",
  "keywords": [
    "next",
    "nextjs",
    "urlSearchParams",
    "typesafe",
    "typescript",
    "zod"
  ],
  "bin": "./dist/cli.js",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./pages": {
      "require": "./dist/pages.js",
      "import": "./dist/pages.mjs"
    },
    "./app": {
      "require": "./dist/app.js",
      "import": "./dist/app.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "pages": [
        "./dist/pages.d.ts"
      ],
      "app": [
        "./dist/app.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup src/cli.ts src/index.ts src/app.ts src/pages.ts --format cjs,esm --dts",
    "lint": "tsc --noEmit",
    "dev": "npm run build -- --watch"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "meow": "9.0.0"
  },
  "devDependencies": {
    "next": "13.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "^6.6.3",
    "typescript": "5.0.4",
    "zod": "^3.20.2"
  },
  "peerDependencies": {
    "next": "^13.2.3",
    "react": "^18.0.0",
    "zod": "^3.20.2"
  }
}

{
  "name": "next-typesafe-url",
  "description": "JSON serializable, fully typesafe, and zod validated URL search params, dynamic route params, and routing for NextJS pages directory",
  "license": "MIT",
  "version": "1.3.4-beta.11",
  "author": "Ethan Niser",
  "repository": {
    "type": "git",
    "url": "https://github.com/ethanniser/next-typesafe-url"
  },
  "keywords": [
    "next",
    "nextjs",
    "urlSearchParams",
    "typesafe",
    "typescript",
    "zod"
  ],
  "bin": "./dist/cli.js",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./server": {
      "require": "./dist/server.js",
      "import": "./dist/server.mjs"
    },
    "./client": {
      "require": "./dist/client.js",
      "import": "./dist/client.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "server": [
        "./dist/server.d.ts"
      ],
      "client": [
        "./dist/client.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup src/cli.ts src/index.ts src/server.ts src/client.ts --format cjs,esm --dts",
    "lint": "tsc --noEmit",
    "dev": "npm run build -- --watch"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "meow": "9.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.2.3",
    "next": "^13.2.3",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "zod": "^3.20.2"
  },
  "peerDependencies": {
    "next": "^13.2.3",
    "react": "^18.0.0",
    "zod": "^3.20.2"
  }
}
